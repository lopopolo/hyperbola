{
  "name": "hyperbola",
  "version": "0.107.0",
  "description": "Build pipeline for Ryan Lopopolo's website - hyperbo.la",
  "keywords": [
    "javascript minification",
    "css minification",
    "asset caching",
    "build pipeline"
  ],
  "homepage": "https://hyperbo.la",
  "bugs": "https://github.com/lopopolo/hyperbola/issues",
  "license": "MIT",
  "author": {
    "name": "Ryan Lopopolo",
    "email": "rjl@hyperbo.la",
    "url": "https://hyperbo.la"
  },
  "contributors": [
    {
      "name": "Ryan Lopopolo",
      "email": "rjl@hyperbo.la",
      "url": "https://hyperbo.la"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lopopolo/hyperbola.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "base64-inline-loader": "^1.1.0",
    "bootstrap-css-only": "^3.3.7",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "glob-all": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-favicons": "^2.2.7",
    "gulp-filter": "^5.0.1",
    "gulp-imagemin": "^3.3.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-svg2png": "^2.0.2",
    "gulp-util": "^3.0.8",
    "ignore-loader": "^0.1.2",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "^2.1.0",
    "run-sequence": "^2.2.0",
    "style-loader": "^0.19.0",
    "stylefmt": "^6.0.0",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "text-to-svg": "^3.1.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-stream": "^4.0.0"
  },
  "dependencies": {
    "retinajs": "^2.1.3",
    "time-elements": "^0.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "impliedStrict": true,
      "sourceType": "module"
    }
  }
}
