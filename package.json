{
  "name": "hyperbola",
  "version": "0.155.0",
  "description": "Build pipeline for Ryan Lopopolo's website - hyperbo.la",
  "keywords": [
    "javascript minification",
    "css minification",
    "asset caching",
    "build pipeline"
  ],
  "homepage": "https://hyperbo.la",
  "bugs": "https://github.com/lopopolo/hyperbola/issues",
  "license": "MIT",
  "author": {
    "name": "Ryan Lopopolo",
    "email": "rjl@hyperbo.la",
    "url": "https://hyperbo.la"
  },
  "contributors": [
    {
      "name": "Ryan Lopopolo",
      "email": "rjl@hyperbo.la",
      "url": "https://hyperbo.la"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lopopolo/hyperbola.git"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.2.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "file-loader": "^4.0.0",
    "glob": "^7.1.3",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.14.6",
    "prettier": "^1.15.3",
    "prettier-eslint": "^9.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^3.0.0",
    "svgo": "^1.0.1",
    "svgo-loader": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^2.0.0",
    "webapp-webpack-plugin": "^2.5.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.0.3"
  },
  "dependencies": {
    "text-to-svg": "^3.1.3"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "extends": [
      "airbnb",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-closing-bracket-location": "off",
      "react/jsx-closing-tag-location": "off",
      "react/jsx-curly-spacing": "off",
      "react/jsx-equals-spacing": "off",
      "react/jsx-first-prop-new-line": "off",
      "react/jsx-indent": "off",
      "react/jsx-indent-props": "off",
      "react/jsx-max-props-per-line": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-multi-spaces": "off",
      "react/jsx-tag-spacing": "off",
      "react/jsx-wrap-multilines": "off",
      "react/self-closing-comp": "off"
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  }
}
